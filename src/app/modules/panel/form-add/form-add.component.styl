@import '../../../../stylus/_variable'

.add-photo
  height 150px
  background color-gray-light-font
  color color-white
  display flex
  align-items center
  justify-content center
  font-size 60px
  margin-bottom 20px
  cursor pointer

button
  display table
  width 95%
  text-transform uppercase
  font-weight bold
  font-size 16px
  color color-blue
  background color-yellow
  padding 10px
  border-radius 10px
  margin 30px auto 0

